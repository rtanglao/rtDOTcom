            <span class="n">id</span>            <span class="nb">INTEGER</span> <span class="k">PRIMARY</span> <span class="k">KEY</span><span class="p">,</span>
            <span class="n">canonical_url</span> <span class="nb">TEXT</span> <span class="k">NOT</span> <span class="k">NULL</span><span class="p">,</span>
            <span class="n">created</span>       <span class="nb">INTEGER</span> <span class="k">NOT</span> <span class="k">NULL</span><span class="p">,</span>
            <span class="n">note_text</span>     <span class="nb">TEXT</span> <span class="k">NOT</span> <span class="k">NULL</span>
          <span class="p">);</span>
